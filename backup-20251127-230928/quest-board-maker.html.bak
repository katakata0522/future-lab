<script>
    // --- Download Source ---
    function downloadSource() {
        const htmlContent = document.documentElement.outerHTML;
        const blob = new Blob([htmlContent], { type: 'text/html' });
        const a = document.createElement('a');
        a.href = URL.createObjectURL(blob);
        a.download = 'quest-board-maker.html';
        document.body.appendChild(a);
        a.click();
        document.body.removeChild(a);
    }

    // --- Quest Board Logic ---
    function updateBoard() {
        const title = document.getElementById('quest-title').value || "クエスト名未設定";
        const count = document.getElementById('quest-count').value;
        const reward = document.getElementById('quest-reward').value || "ごほうび未設定";

        document.getElementById('disp-title').textContent = title;
        document.getElementById('disp-reward').textContent = reward;
        document.getElementById('count-val').textContent = count;

        generateGrid(count);
    }

    function generateGrid(count) {
        const grid = document.getElementById('stamp-grid');
        grid.innerHTML = '';

        // カラム数調整 (数が多い場合は詰める)
        if(count > 20) grid.style.gridTemplateColumns = "repeat(6, 1fr)";
        else if(count > 12) grid.style.gridTemplateColumns = "repeat(5, 1fr)";
        else grid.style.gridTemplateColumns = "repeat(4, 1fr)";

        const iconHtml = document.getElementById('stamp-icon-template').innerHTML;

        for(let i=1; i<=count; i++) {
            const spot = document.createElement('div');
            spot.className = "stamp-spot";
            if(i == count) spot.classList.add('goal');
            
            // 数字
            spot.innerHTML = `<span class="relative z-10">${i}</span><div class="stamp-mark z-20">${iconHtml}</div>`;
            
            // クリックイベント（トグル）
            spot.onclick = function() {
                this.classList.toggle('stamped');
                // 音を鳴らすなどの演出があればここに
            };
            
            grid.appendChild(spot);
        }
    }

    // Init
    window.onload = () => {
        updateBoard();
    };
</script>

</body></html>