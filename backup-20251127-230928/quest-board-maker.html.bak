<meta charset="utf-8">
<meta content="width=device-width, initial-scale=1.0" name="viewport">
<title>Parent's Cafe | おうちクエストボード</title>
<script src="https://cdn.tailwindcss.com"></script>
<link href="https://fonts.googleapis.com/css2?family=Zen+Maru+Gothic:wght@400;500;700;900&amp;display=swap" rel="stylesheet">
<link href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@40,400,0,0" rel="stylesheet">
<style>
    body {
        font-family: 'Zen Maru Gothic', sans-serif;
        background-color: #F5F5F0;
        background-image: url("data:image/svg+xml,%3Csvg width='60' height='60' viewBox='0 0 60 60' xmlns='http://www.w3.org/2000/svg'%3E%3Cg fill='none' fill-rule='evenodd'%3E%3Cg fill='%23E6E6DC' fill-opacity='0.4'%3E%3Cpath d='M36 34v-4h-2v4h-4v2h4v4h2v-4h4v-2h-4zm0-30V0h-2v4h-4v2h4v4h2V6h4V4h-4zM6 34v-4H4v4H0v2h4v4h2v-4h4v-2H6zM6 4V0H4v4H0v2h4v4h2V6h4V4H6z'/%3E%3C/g%3E%3C/g%3E%3C/svg%3E");
        color: #57534E;
        min-height: 100vh;
        display: flex;
        flex-direction: column;
    }

    /* マスコット */
    .maron-sm-wrapper { width: 32px; height: 28px; position: relative; display: inline-block; vertical-align: middle; }
    .maron-body { width: 100%; height: 100%; background-color: #A16207; border-radius: 50% 50% 45% 45%; position: relative; }
    .maron-face { width: 80%; height: 70%; background-color: #FEF3C7; border-radius: 50% 50% 45% 45%; position: absolute; bottom: 5%; left: 10%; }
    .maron-ear { width: 24%; height: 28%; background-color: #A16207; border-radius: 50%; position: absolute; top: -10%; }
    .maron-ear.l { left: 5%; } .maron-ear.r { right: 5%; }
    .maron-eye { width: 8%; height: 12%; background-color: #451A03; border-radius: 50%; position: absolute; top: 38%; }
    .maron-eye.l { left: 32%; } .maron-eye.r { right: 32%; }
    .maron-mouth { width: 16%; height: 8%; border-bottom: 2px solid #451A03; border-radius: 50%; position: absolute; top: 50%; left: 42%; }

    /* クエストボード */
    .quest-card {
        width: 100%; max-width: 600px;
        background: white;
        border: 8px solid #FCD34D; /* Amber 300 */
        border-radius: 20px;
        padding: 30px;
        margin: 0 auto;
        position: relative;
        box-shadow: 0 10px 15px -3px rgba(0,0,0,0.1);
        transition: transform 0.3s;
    }
    .quest-title-area {
        text-align: center;
        border-bottom: 3px dashed #FCD34D;
        padding-bottom: 15px;
        margin-bottom: 20px;
    }
    .stamp-grid {
        display: grid;
        grid-template-columns: repeat(5, 1fr);
        gap: 15px;
        margin-bottom: 25px;
    }
    .stamp-spot {
        aspect-ratio: 1;
        border: 3px dashed #E5E7EB;
        border-radius: 50%;
        display: flex; align-items: center; justify-content: center;
        font-weight: 900; color: #E5E7EB;
        font-size: 1.2rem;
        cursor: pointer;
        transition: all 0.2s;
        position: relative;
        background-color: #F9FAFB;
    }
    .stamp-spot:hover {
        border-color: #FCA5A5; color: #FCA5A5;
    }
    /* スタンプ済み */
    .stamp-spot.stamped {
        border: 3px solid #F87171;
        background-color: #FEF2F2;
        color: transparent; /* 数字を隠す */
    }
    .stamp-mark {
        position: absolute; inset: 0;
        display: none; align-items: center; justify-content: center;
        color: #EF4444; 
        animation: stamp-pop 0.3s cubic-bezier(0.175, 0.885, 0.32, 1.275);
    }
    .stamp-spot.stamped .stamp-mark { display: flex; }
    
    /* ゴール地点の特別スタイル */
    .stamp-spot.goal {
        border-color: #FCD34D;
        background-color: #FFFBEB;
    }
    .stamp-spot.goal::after {
        content: 'GOAL';
        position: absolute; bottom: -20px;
        font-size: 10px; font-weight: bold; color: #F59E0B;
    }

    /* 報酬エリア */
    .reward-area {
        background-color: #FFF7ED;
        border: 2px solid #FED7AA;
        border-radius: 15px;
        padding: 15px;
        display: flex;
        align-items: center;
        gap: 15px;
    }

    @keyframes stamp-pop { 0%{transform: scale(1.5); opacity: 0;} 100%{transform: scale(1); opacity: 1;} }

    /* 印刷設定 */
    @media print {
        @page { margin: 0; size: A4; }
        body { background: white; height: auto; display: block; }
        body > *:not(main) { display: none !important; }
        main { padding: 0 !important; }
        .no-print { display: none !important; }
        
        .quest-container {
            display: block !important;
            width: 100%;
            height: 100%;
            padding: 20mm;
        }
        .quest-card {
            box-shadow: none;
            border-width: 4px; /* インク節約 */
            page-break-inside: avoid;
            transform: scale(1) !important;
        }
    }
</style>
